        </div>
    </div>
</script>

<!-- start of the form temlates (this is just for the form builder // not used inside Ember.js) -->
<script id="temp-address" type="text/x-handlebars-template">
  <div>
	<div class="form-group {{#if required}}required{{/if}}">
		<label for="{{cid}}-{{label}}">{{label}}</label>
		<input type="text"  class="form-control {{form-size}}" id="address-street-{{cid}}-{{label}}" placeholder="Street" name="address-street-{{cid}}-{{label}}" {{#if required}}data-bv-notempty="true" required{{/if}}>
	</div>
	<div class="form-group">
		<input type="text"  class="form-control {{form-size}}" id="address-city-{{cid}}-{{label}}" placeholder="City" name="address-city-{{cid}}-{{label}}" {{#if required}}data-bv-notempty="true" required{{/if}}>
	</div>
	<div class="form-group">
		<input type="text"  class="form-control {{form-size}}" id="address-state-{{cid}}-{{label}}" placeholder="State" name="address-state-{{cid}}-{{label}}" {{#if required}}data-bv-notempty="true" required{{/if}}>
	</div>
	<div class="form-group">
		<input type="text"  class="form-control {{form-size}}" id="address-postcode-{{cid}}-{{label}}" placeholder="Postcode" name="address-postcode-{{cid}}-{{label}}" {{#if required}}data-bv-notempty="true" required{{/if}}>
	</div>
	<div class="form-group">
		<input type="text"  class="form-control {{form-size}}" id="address-country-{{cid}}-{{label}}" placeholder="Country" name="address-country-{{cid}}-{{label}}" {{#if required}}data-bv-notempty="true" required{{/if}}>
	</div>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-time" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>
    <input type="text" pattern="^([01]?[0-9]|2[0-3]):[0-5][0-9]$" placeholder="hh:mm" data-bv-regexp-message="Please enter a time in the correct format (Ex. 13:52)" class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-date" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>
    <input type="text" data-bv-date="true" data-bv-date-format="DD/MM/YYYY" placeholder="DD/MM/YYYY" class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-website" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>
    <input type="text" data-bv-uri="true" data-bv-uri-message="Please enter a valid URL starting with http://" class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-number" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>
    <input type="text"class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" min="{{field_options.min}}" max="{{field_options.max}}" {{#if field_options.integer_only}}data-bv-integer="true" data-bv-integer-message="Number needs to be an integer. '1' is okay, '1.1' not!"{{/if}} {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
	{{field_options.units}}
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-price" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>
    $<input type="text" data-bv-greaterthan="true" data-bv-greaterthan-value="0" data-bv-greaterthan-inclusive='false' class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-email" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}} control-label">{{label}}</label>
    <input type="email" class="form-control {{form-size}}" id="{{cid}}-{{label}}" name="{{cid}}-{{label}}" data-bv-emailaddress="true" data-bv-emailaddress-message="Please enter a valid email in the form of name@mail.com" {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
    <span class="help-block">{{field_options.description}}</span>
  </div>
</script>

<script id="temp-radio" type="text/x-handlebars-template">
<div class="form-group {{#if field_options.required}}required{{/if}}">
 <label for="{{cid}}-{{label}}">{{label}}</label>

   {{#each field_options.options}}
     <div class="radio">
        <label>
          <input type="radio" name="{{../label}}" id="{{../cid}}" {{#if field_options.required}}data-bv-notempty="true"{{/if}} value="{{label}}" {{#if checked}} checked{{/if}}>
          {{label}}
        </label>
      </div>

   {{/each}}

 <span class="help-block">{{field_options.description}}</span>
</div>
</script>


<script id="temp-checkboxes" type="text/x-handlebars-template">
  <div class="form-group {{#if field_options.required}}required{{/if}}">
    <label for="{{cid}}-{{label}}">{{label}}</label>

      {{#each field_options.options}}
        <div class="checkbox">
          <label>
            <input name="{{../cid}}-{{label}}" id="{{../cid}}-{{label}}" type="checkbox" {{#if checked}}checked{{/if}} {{#if field_options.required}}data-bv-notempty="true"{{/if}}>
            {{label}}
          </label>
        </div>
      {{/each}}

    <span class="help-block">{{field_options.description}}</span>
   </div>
</script>

