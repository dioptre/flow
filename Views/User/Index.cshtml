<script type="text/x-handlebars" id="permission">
    <h1> My Permissions </h1>
    <br />
    {{view Ember.Select content=types optionValuePath="content.id"  optionLabelPath="content.text"  value=type}}
    <br /><br />
    <div class='container-fluid'>
        <div class="row">
            <div class="col-md-12">
                <b>{{test}}</b>

                {{#if model}}
                    <div class="table-responsive">
                        <table class="table table-condensed table-hover">
                            <thead>
                            <tr>
                                {{!--<th>Preview</th>--}}
                                <th>Name</th>
                                <th>Updated</th>
                                <th>Company</th>
                                <th>Assignee</th>
                                {{#if controllers.application.isLoggedIn}}
                                <th>Permissions</th>
                                {{/if}}
                            </tr>
                            </thead>
                            <tbody>
                            {{#each item in model}}
                                <tr>
                                {{!--<td><img {{bind-attr src=result.ResourcePreviewPath}} height="15"></td>--}}
                                <td>{{item.ReferenceName}}</td>
                                <td>{{item.Updated}}</td>
                                <td>
                                        {{item.AccessorCompanyName}}
                                </td>
                                <td>

                                        {{item.AccessorContactName}}

                                </td>
                                {{#if controllers.application.isLoggedIn}}
                                    <td><a {{action 'deletePermission' item}} class="hlink">Delete</a></td>
                                {{/if}}
                                </tr>
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                {{else}}
                    <br />
                    None.
                {{/if}}
            </div>
        </div>
     </div>
</script>

<script type="text/x-handlebars" id="myworkflows">
    <h3> My Workflows & Processes</h3>
    <br><br>
    {{#if loading}}
      Loading...
    {{else}}
      {{#if model}}
        {{#eui-modal openBinding="permissionModal"}}
            <div style="padding: 50px; text-align: center;">
                <h4>Edit Permissions</h4>

                <br>
                <p> You can grant other users and companies access to your workflow. <br>To add them use the input box below. </p>
                <br>

                <b>Add user:</b><br>
                <input id="add-users-perm" style="width: 275px;" class='select2' type='hidden'/>

                <br>
                <br>

                <b>Add company:</b><br>
                <input id="add-comp-perm" style="width: 275px;" class='select2' type='hidden'/>


                <br>
                <br>

                {{eui-button label="Cancel" action="cancelPermission" size='medium'}}
                {{eui-button label="Add" action="submitPermission" style='primary' size='medium'}}
            </div>
        {{/eui-modal}}

        <div class='container-fluid'>
            <div class="row">
                <div class="col-md-6">
                    {{model.workflows.length}} <b>Workflows</b>

                    {{#if model.workflows}}
                        <div class="table-responsive">
                          <table class="table table-condensed table-hover">
                              <thead>
                                <tr>
                                  {{!--<th>Preview</th>--}}
                                  <th>Name</th>
                                  <th>Updated</th>
                                  <th>Link</th>
                                  {{#if controllers.application.isLoggedIn}}
                                    <th>Permissions</th>
                                  {{/if}}
                                </tr>
                              </thead>
                              <tbody>
                                {{#each workf in model.workflows}}
                                  <tr>
                                    {{!--<td><img {{bind-attr src=result.ResourcePreviewPath}} height="15"></td>--}}
                                    <td>{{workf.humanName}}</td>
                                    <td>{{workf.Updated}}</td>
                                    <td>{{link-to 'View' 'workflow' workf.id}}</td>
                                    {{#if controllers.application.isLoggedIn}}
                                        <td><a {{action 'editPermission' workf}} class="hlink">Edit</a></td>
                                    {{/if}}
                                  </tr>
                                {{/each}}
                              </tbody>
                          </table>
                        </div>
                    {{else}}
                        You have not created any workflows yet.
                    {{/if}}
                </div>

                <div class="col-md-6">
                    {{model.processes.length}} <b>Processes</b>

                    {{#if model.processes}}
                        <div class="table-responsive">
                            <table class="table table-condensed table-hover">
                              <thead>
                                <tr>
                                  {{!--<th>Preview</th>--}}
                                  <th>Name</th>
                                  <th>Updated</th>
                                  <th>Link</th>
                                  {{#if controllers.application.isLoggedIn}}
                                    <th>Permissions</th>
                                  {{/if}}
                                </tr>
                              </thead>
                              <tbody>
                                {{#each proc in model.processes}}
                                  <tr>
                                    {{!--<td><img {{bind-attr src=proc.ResourcePreviewPath}} height="15"></td>--}}
                                    <td>{{proc.Title}}</td>
                                    <td>{{proc.Updated}}</td>
                                    <td>{{link-to 'View' 'graph' proc.id}}</td>
                                    {{#if controllers.application.isLoggedIn}}
                                        <td><a {{action 'editPermission' proc}} class="hlink">Edit</a></td>
                                    {{/if}}
                                  </tr>
                                {{/each}}
                              </tbody>
                          </table>
                        </div>
                    {{else}}
                        You have not created any processes yet.
                    {{/if}}
                </div>
            </div>
        </div>

      {{else}}
            {{#if controllers.application.isLoggedIn}}
                You have no files.
            {{else}}
                You are not logged in. Please {{link-to 'login' 'login'}} to see your files.
            {{/if}}

      {{/if}}
    {{/if}}
</script>


<script type="text/x-handlebars" id="file">
    <h1> My Files </h1>
    <br><br>
    {{#if loading}}
      Loading...
    {{else}}
      {{#if model}}
        {{#eui-modal openBinding="permissionModal"}}
            <div style="padding: 50px; text-align: center;">
                <h4>Edit Permissions</h4>

                <br>
                <p> You can grant other users and companies access to your file. <br>To add them use the input box below. </p>
                <br>

                <b>Add user:</b><br>
                <input id="add-users-perm" style="width: 275px;" class='select2' type='hidden'/>

                <br>
                <br>

                <b>Add company:</b><br>
                <input id="add-comp-perm" style="width: 275px;" class='select2' type='hidden'/>


                <br>
                <br>

                {{eui-button label="Cancel" action="cancelPermission" size='medium'}}
                {{eui-button label="Add" action="submitPermission" style='primary' size='medium'}}
            </div>
        {{/eui-modal}}
        <div class="table-responsive">
          <table class="table table-condensed table-hover">
              <thead>
                <tr>
                  <th>Preview</th>
                  <th>Title</th>
                  <th>Description</th>
                  <th>Updated</th>
                  <th>Link</th>
                  {{#if controllers.application.isLoggedIn}}
                    <th>Permissions</th>
                  {{/if}}
                </tr>
              </thead>
              <tbody>
                {{#each result in model}}
                  <tr>
                    <td><img {{bind-attr src=result.ResourcePreviewPath}} height="15"></td>
                    <td>{{result.Title}}</td>
                    <td>{{result.Description}}</td>
                    <td>{{result.Updated}}</td>
                    <td><a {{bind-attr href=result.ResourcePath}}>Download File</a></td>
                    {{#if controllers.application.isLoggedIn}}
                        <td><a {{action 'editPermission' result}} class="hlink">Edit</a></td>
                    {{/if}}
                  </tr>
                {{/each}}
              </tbody>
          </table>
        </div>

      {{else}}
            {{#if controllers.application.isLoggedIn}}
