<script type="text/x-handlebars" id="signup">
    <div class="login-helper login-helper-signup"></div>
    <div class="login-wrapper login-bg captcha">
        <a href="#">
            <img class="logo" src="@Url.Content("~/Modules/EXPEDIT.Flow/Images/logo-all-white-transparent.png")" alt="logo" />
        </a>
        <form {{action 'signupUser' on='submit'}} >
        <div class="box">
            <div class="content-wrap">

                <h6>Sign up</h6>
                <p>
                    Welcome to FlowPro. To sign up as a free user (no credit card required), just fill out the form below. Upgrading to a premium user, can be done anytime after that.
                </p>
                {{#unless controllers.application.isLoggedIn}}
                    {{input type="text" valueBinding='username' classNameBindings=":form-control" placeholder="Username"}}
                    {{input type="text" valueBinding='email' classNameBindings=":form-control" placeholder="E-mail address"}}
                    {{input valueBinding='password' type="password" classNameBindings=":form-control" placeholder="Choose a Password"}}

                    <h5> Verify you're human:</h5>
                    <img class='captcha-img' alt="Embedded Image" {{bind-attr src=captchaURL}} width="100" height="40" />
                    <div class="refresh" title="Create a new human code" {{action 'changeCaptcha'}}>
                        <i class="icon-refresh" ></i>
                    </div>
                    <div style='width: 100px; display: inline-block;'>
                        {{input valueBinding='captchaSolution' type="text" classNameBindings=":form-control" placeholder="Characters above"}}
                    </div>

                    <button type="submit" class="btn-glow primary login">
                        Sign Up
                    </button>
                {{else}}
                    <p>You are already logged in as <b>{{controllers.application.userProfile.UserName}}</b></p>
                    <p>Log out to sign up as new user.</p>
                {{/unless}}

            </div>
        </div>
        </form>
        <div class="no-account">
            <p>Already have an account?</p>
            {{#link-to 'login'}}Log in{{/link-to}}
        </div>
    </div>

</script>


<script type="text/x-handlebars" id="login">
    <div class="login-helper"></div>
    <div class="login-wrapper login-bg">
        <a href="#">
            <img class="logo" src="@Url.Content("~/Modules/EXPEDIT.Flow/Images/logo-all-white-transparent.png")" alt="logo" />
        </a>
        <form {{action 'loginUser' on='submit'}} >
        <div class="box">
            <div class="content-wrap">
                <h6>Log in</h6>

                 {{#unless controllers.application.isLoggedIn}}
                    {{input type="text" valueBinding='email' classNameBindings=":form-control" placeholder="E-mail address"}}
                    {{input valueBinding='password' type="password" class="form-control" placeholder="Your Password"}}
                    {{#link-to 'resetpassword' class="forgot"}}Forgot password?{{/link-to}}
                    <div class="remember">
                        {{input valueBinding="rememberme" id="remember-me" type="checkbox"}}
                        <label for="remember-me">Remember me</label>
                    </div>
                    <button type="submit" class="btn-glow primary login">
                        Log in
                    </button>
                {{else}}
                    <p>You are already logged in as <b>{{controllers.application.userProfile.UserName}}</b></p>
                    <p>Log out to log in as a different user.</p>
                {{/unless}}
            </div>
        </div>
        </form>
        <div class="no-account">
            <p>Do not have an account?</p>
            {{#link-to 'signup'}}Sign up{{/link-to}}
        </div>
    </div>
</script>

