<script type="text/x-handlebars" id="login">

    <div class="login-wrapper login-bg">
        <a href="#">
            <img class="logo" src="@Url.Content("~/Modules/EXPEDIT.Flow/Images/logo-all-white-transparent.png")" alt="logo" />
        </a>
        <form {{action 'loginUser' on='submit'}} >
        <div class="box">
            <div class="content-wrap">
                <h6>Log in</h6>

                {{input type="text" valueBinding='email' classNameBindings=":form-control" placeholder="E-mail address"}}
                {{input valueBinding='password' type="password" class="form-control" placeholder="Your Password"}}
                <a href="/Users/account/RequestLostPassword" class="forgot">Forgot password?</a>
                <div class="remember">
                    {{input valueBinding="rememberme" id="remember-me" type="checkbox"}}
                    <label for="remember-me">Remember me</label>
                </div>
                <button type="submit" class="btn-glow primary login">
                    Log in
                </button>
            </div>
        </div>
        </form>
        <div class="no-account">
            <p>Do not have an account?</p>
            <a href="/Users/account/Register?ReturnUrl=%2flow">Sign up</a>
        </div>
    </div>

</script>


