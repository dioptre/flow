        <script type="text/x-handlebars" id="mylicenses">
            <h3> My Licenses @*<small><a href="/store/">Buy License</a></small>*@</h3>
            <br><br>
            {{#if loading}}
              Loading...
            {{else}}
             {{#if controllers.application.isLoggedIn}}
              {{#if model}}
                {{#eui-modal openBinding="userModal"}}
                    <div style="padding: 50px; text-align: center;">
                        <h4>Reassign License</h4>

                        <br>
                        <p> You can reassign the selected license to a different user. <br>Update it using the input box below. </p>
                        <br>

                        <b>Assign to user:</b><br>
                        <input id="add-user-perm" style="width: 275px;" class='select2' type='hidden'/>

                        <br>
                        <br>

                        {{eui-button label="Cancel" action="cancelPermission" size='medium'}}
                        {{eui-button label="Update" action="submitPermission" style='primary' size='medium'}}
                    </div>
                {{/eui-modal}}

                <div class='container-fluid'>
                    <div class="row">
                        <div class="col-md-12">
                            {{model.licenses.length}} <b>Licenses</b>

                            {{#if model.licenses}}
                                <div class="table-responsive">
                                  <table class="table table-condensed table-hover">
                                      <thead>
                                        <tr>
                                          {{!--<th>Preview</th>--}}
                                          <th>Product Name</th>
                                          <th>License ID</th>
                                          <th>Assignee</th>
                                          <th>Valid From</th>
                                          <th>Expiry</th>
                                          {{#if controllers.application.isLoggedIn}}
                                            <th>Reassign</th>
                                          {{/if}}
                                        </tr>
                                      </thead>
                                      <tbody>
                                        {{#each lic in model.licenses}}
                                          <tr {{bind-attr class="lic.IsExpired:warning"}}>
                                            <td>{{lic.ModelName}}</td>
                                            <td>{{lic.id}}</td>
                                            <td>{{{lic.LicenseeUsernameTags}}}</td>
                                            <td>{{lic.ValidFromNice}}</td>
                                            <td>{{lic.ExpiryNice}}</td>
                                            {{#if controllers.application.isLoggedIn}}
                                                <td><a {{action 'editPermission' lic}} class="hlink">Edit</a></td>
                                            {{/if}}
                                          </tr>
                                        {{/each}}
                                      </tbody>
                                  </table>
                                </div>
                            {{else}}
                                You have not purchased any licenses yet.
                            {{/if}}
                        </div>
                    </div>
                </div>
                {{else}}
                 You have no licenses.
              {{/if}}
            {{else}}
                You are not logged in. Please {{link-to 'login' 'login'}} to see your licenses.
            {{/if}}
          {{/if}}
       </script>

        <div id="gout"></div>
    </body>
