                  <td>{{safehtml result.Updated}}</td>
                  <td><a {{bind-attr href=result.ResourcePath}}>Download File</a></td>
                </tr>
              {{/each}}
            </tbody>
        </table>
      </div>

           </div>

    {{else}}

        Sorry no results could be found.

    {{/if}}


  {{/if}}

</script>



