<script id="form-tabs" type="text/x-handlebars-template">
    <ul class="nav nav-tabs" role="tablist">
        <li class="active"><a href="#form-{{id}}">{{#if heading }}{{heading}} {{/if}}Form</a></li>
        <li><a href="#results-{{id}}" class="results">Results</a></li>
