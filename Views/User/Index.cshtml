<script type="text/x-handlebars" id="myworkflows">
    <h3> My Workflows & Processes</h3>
    <br><br>
    {{#if loading}}
      Loading...
    {{else}}
     {{#if controllers.application.isLoggedIn}}
      {{#if model}}
        {{#eui-modal openBinding="permissionModal"}}
            <div style="padding: 50px; text-align: center;">
                <h4>Edit Permissions</h4>

                <br>
                <p> You can grant other users and companies access to your workflow. <br>To add them use the input box below. </p>
                <br>

                <b>Add user:</b><br>
                <input id="add-users-perm" style="width: 275px;" class='select2' type='hidden'/>

                <br>
                <br>

                <b>Add company:</b><br>
                <input id="add-comp-perm" style="width: 275px;" class='select2' type='hidden'/>


                <br>
                <br>

                {{eui-button label="Cancel" action="cancelPermission" size='medium'}}
                {{eui-button label="Add" action="submitPermission" style='primary' size='medium'}}
            </div>
        {{/eui-modal}}

        <div class='container-fluid'>
            <div class="row">
                <div class="col-md-6">
                    {{model.workflows.length}} <b>Workflows</b>

                    {{#if model.workflows}}
                        <div class="table-responsive">
                          <table class="table table-condensed table-hover">
                              <thead>
                                <tr>
                                  {{!--<th>Preview</th>--}}
                                  <th>Name</th>
                                  <th>Updated</th>
                                  <th>Link</th>
                                  {{#if controllers.application.isLoggedIn}}
                                    <th>Permissions</th>
                                  {{/if}}
                                </tr>
                              </thead>
                              <tbody>
                                {{#each workf in model.workflows}}
                                  <tr>
                                    {{!--<td><img {{bind-attr src=result.ResourcePreviewPath}} height="15"></td>--}}
                                    <td>{{workf.humanName}}</td>
                                    <td>{{workf.updatedNiceMinutes}}</td>
                                    <td>{{link-to 'View' 'workflow' workf.id}}</td>
                                    {{#if controllers.application.isLoggedIn}}
                                        <td><a {{action 'editPermission' workf}} class="hlink">Edit</a></td>
                                    {{/if}}
                                  </tr>
                                {{/each}}
                              </tbody>
                          </table>
                        </div>
                    {{else}}
                        You have not created any workflows yet.
                    {{/if}}
                </div>

                <div class="col-md-6">
                    {{model.processes.length}} <b>Processes</b>

                    {{#if model.processes}}
                        <div class="table-responsive">
                            <table class="table table-condensed table-hover">
                              <thead>
                                <tr>
                                  {{!--<th>Preview</th>--}}
                                  <th>Name</th>
                                  <th>Updated</th>
                                  <th>Link</th>
                                  {{#if controllers.application.isLoggedIn}}
                                    <th>Permissions</th>
                                  {{/if}}
                                </tr>
                              </thead>
                              <tbody>
                                {{#each proc in model.processes}}
                                  <tr>
                                    {{!--<td><img {{bind-attr src=proc.ResourcePreviewPath}} height="15"></td>--}}
                                    <td>{{proc.humanName}}</td>
                                    <td>{{proc.updatedNiceMinutes}}</td>
                                    <td>{{link-to 'View' 'graph' proc.id (query-params workflowID="undefined")}}</td>
                                    {{#if controllers.application.isLoggedIn}}
                                        <td><a {{action 'editPermission' proc}} class="hlink">Edit</a></td>
                                    {{/if}}
                                  </tr>
                                {{/each}}
                              </tbody>
                          </table>
                        </div>
                    {{else}}
                        You have not created any processes yet.
                    {{/if}}
                </div>
            </div>
        </div>
        {{else}}
         You have no files.
      {{/if}}
      {{else}}
           You are not logged in. Please {{link-to 'login' 'login'}} to see your workflows and processes.
      {{/if}}
    {{/if}}
</script>


